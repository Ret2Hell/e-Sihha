{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/e-sehha/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/e-sehha/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/e-sehha",
  "projects": {
    "e-sehha": {
      "type": "application",
      "root": "apps/e-sehha",
      "entryFile": "main",
      "sourceRoot": "apps/e-sehha/src",
      "compilerOptions": {
        "tsConfigPath": "apps/e-sehha/tsconfig.app.json"
      }
    },
    "patients": {
      "type": "application",
      "root": "apps/patients",
      "entryFile": "main",
      "sourceRoot": "apps/patients/src",
      "compilerOptions": {
        "tsConfigPath": "apps/patients/tsconfig.app.json"
      }
    },
    "shared-auth": {
      "type": "library",
      "root": "libs/shared-auth",
      "entryFile": "index",
      "sourceRoot": "libs/shared-auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-auth/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}